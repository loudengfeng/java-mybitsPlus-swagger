<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.ShopDao">

    <resultMap type="com.example.demo.entity.Shop" id="ShopMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="shoptitle" column="shopTitle" jdbcType="VARCHAR"/>
        <result property="price" column="price" jdbcType="NUMERIC"/>
        <result property="shopnum" column="shopNum" jdbcType="INTEGER"/>
    </resultMap>

</mapper>

